               <Font name="Arial Nova Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets bottom="40.0" left="130.0" top="15.0" />
            </VBox.margin>
         </Button>
         <Button fx:id="Orderbtn" mnemonicParsing="false" onAction="#TakeOrders" opacity="0.52" prefHeight="50.0" prefWidth="143.0" style="-fx-background-color: red;" text="Take Orders" textAlignment="CENTER" textFill="#ed9f3f">
            <font>
               <Font name="Arial Nova Bold" size="14.0" />
            </font>
         </Button>
      </children>
      <padding>
         <Insets left="15.0" top="40.0" />
      </padding>
   </VBox>
    <AnchorPane layoutX="1.0" prefHeight="500.0" prefWidth="178.0" style="-fx-background-color: #FFD190;" />

    <ImageView fx:id="logo" fitHeight="162.0" fitWidth="500.0" focusTraversable="true" opacity="0.55" pickOnBounds="true" preserveRatio="true">
        <viewport>
            <Rectangle2D />
        </viewport>
        <image>
            <Image url="@../../../Shopping_Project_Images/logo.png" />


        </image>
    </ImageView>
   <ImageView fitHeight="139.0" fitWidth="146.0" layoutX="250.0" layoutY="4.0" opacity="0.16" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../../../Shopping_Project_Images/red-star.png" />
      </image>
   </ImageView>


</Pane>
