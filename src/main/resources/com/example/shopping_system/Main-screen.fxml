<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.shopping_system.LoginController">
   <VBox layoutX="193.0" layoutY="172.0" prefHeight="319.0" prefWidth="297.0">
      <children>
         <Button fx:id="CheckOrdersbtn" layoutX="25.0" layoutY="50.0" mnemonicParsing="false" onAction="#CheckOrders" opacity="0.51" prefHeight="50.0" prefWidth="143.0" style="-fx-background-color: red;" text="Check Orders" textAlignment="CENTER" textFill="#ed9f3f">
            <font>
               <Font name="Arial Nova Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets bottom="40.0" left="130.0" top="20.0" />
            </VBox.margin>
         </Button>
         <Button fx:id="CheckSalesbtn" layoutX="25.0" layoutY="108.0" mnemonicParsing="false" onAction="#CheckSales" opacity="0.56" prefHeight="47.0" prefWidth="143.0" style="-fx-background-color: red;" text="Review Sales" textAlignment="CENTER" textFill="#ed9f3f">
            <font>
               <Font name="Arial Nova Bold" size="14.0" />
            </font>
            <VBox.margin>
               <Insets top="-25.0" />
            </VBox.margin>
         </Button>
         <Button fx:id="UpdateProductsbtn" layoutX="25.0" layoutY="166.0" mnemonicParsing="false" onAction="#UpdateProducts" opacity="0.52" prefHeight="52.0" prefWidth="143.0" style="-fx-background-color: red;" text="UpdateProducts" textAlignment="CENTER" textFill="#ed9f3f">
