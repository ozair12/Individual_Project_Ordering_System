<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<Pane prefHeight="650.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.shopping_system.LoginController">
    <AnchorPane layoutX="1.0" prefHeight="450.0" prefWidth="166.0" style="-fx-background-color: #FFD190;"/>
    <ImageView fx:id="logo" fitHeight="167.0" fitWidth="452.0" layoutY="2.0" opacity="0.55" pickOnBounds="true"
               preserveRatio="true">
        <viewport>
            <Rectangle2D/>
        </viewport>
        <image>
            <Image url="@../../../Shopping_Project_Images/logo.png"/>


        </image>
    </ImageView>

    <ImageView fitHeight="147.0" fitWidth="200.0" layoutX="216.0" opacity="0.25" pickOnBounds="true"
               preserveRatio="true">
        <image>
            <Image url="@../../../Shopping_Project_Images/red-star.png"/>
        </image>
    </ImageView>
    <BorderPane fx:id="mainpane" prefHeight="200.0" prefWidth="200.0"/>
    <TabPane layoutX="154.0" layoutY="186.0" prefHeight="335.0" prefWidth="525.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
            <Tab text="Pizza"/>
            <Tab text="Meal Deals"/>
            <Tab text="Burger"/>
            <Tab text="Drinks"/>
            <Tab text="Sides"/>
        </tabs>
    </TabPane>


</Pane>
